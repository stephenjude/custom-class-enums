{
    "name": "stephenjude/enums",
    "description": "Custom Type Enum Constants",
    "keywords": [
        "stephenjude",
        "enums"
    ],
    "homepage": "https://github.com/stephenjude/enums",
    "license": "MIT",
    "authors": [
        {
            "name": "stephenjude",
            "email": "hello@stephenjude.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.17",
        "phpunit/phpunit": "^9.5",
        "spatie/ray": "^1.10",
        "vimeo/psalm": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "Stephenjude\\Enums\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Stephenjude\\Enums\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
